{
    "coc.preferences.rootPatterns": [
        ".eslintrc.json",
        "package.json",
        ".git",
        "babelrc-lint.json"
    ],
    "eslint.format.enabled": true,
    "eslint.filetypes": [
        "javascript"
    ],
    "clangd.fallbackFlags": ["-std=c++17"],
    "suggest.noselect": true,
    "suggest.echodocSupport": true,
    "suggest.maxCompleteItemCount": 40,
    "tsserver.log": "verbose",
    "tsserver.npm": "/usr/local/bin/npm",
    "tsserver.enableJavascript": true,
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "javascript.suggest.names": false,
    "javascript.suggest.completeJSDocs": true,
    "javascript.suggest.autoImports": true,
    "eslint.options": {
        "parserOptions": {
            "babelOptions": {
                "cwd": "/Users/nyxtom/netcuras/node-core/"
            }
        }
    },
    "eslint.workingDirectories": [
        { "mode": "auto" }
    ],
    "rust-analyzer.debug.runtime": "vimspector",
    "rust-analyzer.debug.vimspector.configuration.name": "rust - lldb",
    "rust-analyzer.cargo.autoreload": true,
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.inlayHints.typeHints.enable": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.procMacro.attributes.enable": true
}
